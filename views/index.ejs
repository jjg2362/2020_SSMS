  <!-- //공지사항 영역 -->

  <!-- 프로젝트 영역 -->
  <section id="project">
    <div class="content-wrapper">
      <div class="row">
        <h1 class="ir_so">프로젝트 영역입니다</h1>
        <div class="project">
          <h2 class="title">Project List</h2>

          <% var comple = new Array(); %>
        <% var ing = new Array(); %>
        <% var apply = new Array(); %>
        <% var sta_sort = new Array() ;%>

        <% for(var i = 0; i < ProjectList.length; i++) { %>
          <% if(moment(curDate).diff(moment(ProjectList[i].strt_date).format('YYYY-MM-DD'),"months") >= 4 && !ProjectList[i].team_id) { %>
            <% continue; %>
          <% } %>
          <% if((moment(curDate).format('YYYY-MM-DD') >= moment(ProjectList[i].cls_date).format('YYYY-MM-DD'))) { %>
              <% comple.push(i) ; %>
          <% } else if(ProjectList[i].team_id) { %>
              <% ing.push(i) ; %>
          <% } else if(!ProjectList[i].team_id){ %>
               <% apply.push(i) ; %>
               <% sta_sort=apply ; %>
          <% } %>
        <% } %>

        <% for(var i = 0; i < ing.length; i++) { %>
            <% sta_sort.push(ing[i]) ; %>
        <% } %>

        <% for(var i = 0; i < comple.length; i++) { %>
            <% sta_sort.push(comple[i]) ; %>
        <% } %>
  
            <div class="card-group">
            <% for(var i = 0; i < sta_sort.length; i++) { %>
                 <div class="card-item">
                   <div class="card-body">
                       <% if(typeof userInfo != 'undefined' && typeof userInfo.userId != 'undefined') { %>
                       <h6 class="card-title">
                         <a href="/pj/<%= ProjectList[sta_sort[i]].prj_id %>"> <%= ProjectList[sta_sort[i]].prj_name %> 
                        </a>
                      </h6>
                      <%} else{%>
                         <h6 class="card-title"><a href="/mbrmgt/DGU121"> 
                          <%= ProjectList[sta_sort[i]].prj_name %>
                        </a> 
                      </h6>
                      <%}%>
                       <p class="card-text">
                         <%= ProjectList[sta_sort[i]].prj_content %>
                       </p>
                     </div>
  
                     <div class="card-icon">
                       <% if(moment(curDate).format('YYYY-MM-DD') >= moment(ProjectList[sta_sort[i]].cls_date).format('YYYY-MM-DD')) { %>
                            <img src="/image/t3.jpg" style="width: 55px; height: 55px;"/>
                       <% } else if(ProjectList[sta_sort[i]].team_id) { %>
                            <img src="/image/t2.jpg" style="width: 55px; height: 55px;"/>
                       <% } else if(!ProjectList[sta_sort[i]].team_id){ %>
                            <img src="/image/t1.jpg" style="width: 55px; height: 55px;"/>
                       <% } %>
                   </div>

                   <div class="card-body">
                       <p>
                         회사 : &nbsp <%= ProjectList[sta_sort[i]].company_name %>
                       </p>
  
                       <p>
                         멘토 : &nbsp <%= ProjectList[sta_sort[i]].mentor_name %>
                       </p>
                       <div class="choice">
                         <p>
                             <em>
                               1지망 : <%= ProjectList[sta_sort[i]].pc1 %> 팀
                              </em>
                              , 2지망 : <%= ProjectList[sta_sort[i]].pc2 %> 팀, 3지망 : <%= ProjectList[sta_sort[i]].pc3 %> 팀
                            </p>
                        </div>
                       <p>
                         분류 : <%= ProjectList[sta_sort[i]].term_chk %>
                       </p>
                   </div>

                   <div class="card-body">
                     <div class="keywords">
                     <% if(ProjectList[i].keyword1 != null &&  ProjectList[i].keyword1 != '') { %>
                          <a href="#">
                            #<%= ProjectList[sta_sort[i]].keyword1 %>
                          </a>
                          &nbsp &nbsp
                     <% } %>
                     <% if(ProjectList[i].keyword2 != null && ProjectList[i].keyword2 != '') { %>
                          <a href="#"> 
                            #<%= ProjectList[sta_sort[i]].keyword2 %>
                          </a>
                          &nbsp &nbsp
                     <% } %>
                     <% if(ProjectList[i].keyword3 != null && ProjectList[i].keyword3 != '') { %>
                          <a href="#"> 
                            #<%= ProjectList[sta_sort[i]].keyword3 %> 
                          </a>
                     <% } %>
                     &nbsp
                     </div>
                     <% if(ProjectList[i].recommended_Prof != null) { %>
                      <p>추천교수 : <%= ProjectList[sta_sort[i]].recommended_Prof %> </p>
                 <% } %>
                 &nbsp
                   </div>
  
                 </div>
            <% } %>
  
            <input type="hidden" value="<%= userInfo %>" id="navBarStatus">  </input>
  
        </div>
      </div>
  
        </div>
      </div>
    </div>
  </section>
  <!-- //프로젝트 영역 -->

  <!-- 맨위로 가기 버튼 -->
