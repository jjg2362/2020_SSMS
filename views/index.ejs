  <div id="modal">
    <div class="modal_footer">
      <label for="today_check">오늘 하루 보지 않기</label>
      <input type="checkbox" id="today_check" name="today_check" />
      <button class="modal_btn_close">
        <i class="fa fa-times" aria-hidden="true"></i>
      </button>
    </div>
    <div class="modal_header">
    <img src="/image/modal.png"/>    
  </div>
