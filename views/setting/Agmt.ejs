                  <hr class="mt-2">
                  <p class="card-text small">
                    <div>
                      <textarea class="form-control" name="join" id="join" type="text" rows = "7" ><%=AgmtList[0].join%></textarea>
                    </div>
                  </p>
                </div>
              </div>
            </div>
          </div>
          <div class="card-body">
